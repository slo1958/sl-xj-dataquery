#tag WindowCode
	#tag Event
		Sub Opening()
		  PagePanel1.SelectedPanelIndex=0
		  stpg.caption=str(PagePanel1.SelectedPanelIndex+1)+"/"+str(PagePanel1.PanelCount)
		  
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events BevelButton1
	#tag Event
		Sub Pressed()
		  dim p as integer
		  p=PagePanel1.SelectedPanelIndex
		  if p>0 then 
		    PagePanel1.SelectedPanelIndex=p-1
		  else
		    PagePanel1.SelectedPanelIndex=PagePanel1.PanelCount -1
		  end if
		  
		  stpg.caption=str(PagePanel1.SelectedPanelIndex+1)+"/"+str(PagePanel1.PanelCount)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events BevelButton2
	#tag Event
		Sub Pressed()
		  dim p as integer
		  p=PagePanel1.SelectedPanelIndex
		  if p<PagePanel1.PanelCount  then 
		    PagePanel1.SelectedPanelIndex=p+1
		  else
		    PagePanel1.SelectedPanelIndex=0
		  end if
		  
		  stpg.caption=str(PagePanel1.SelectedPanelIndex+1)+"/"+str(PagePanel1.PanelCount)
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
